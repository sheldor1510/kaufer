{
    "name": "Kaufer",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Kaufer",
        "default_popup": "popup.html",
        "default_icon": "./logo.png"
    },
    "icons": {
        "300": "./logo.png"
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ]
}