{
    "name": "Kaufer",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Kaufer",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}